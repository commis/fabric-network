# chaincode configuration
LOCAL_GOPATH=$GOPATH
CHAINCODE_NAME=myfoo
CHAINCODE_VERSION=v1527040507
CHAINCODE_INIT_ARGS={"Args":["init"]}
CHAINCODE_INVOKE_ARGS={"Args":["invoke","admin"]}

CHAINCODE_PACKAGE_REMOTE=energy.com/blockchain/chaincode
