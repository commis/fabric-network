# node IP address
ADDR_CLI=192.168.1.30
ADDR_ORG_1=192.168.1.30
ADDR_ORG_2=192.168.1.30
#ADDR_ORG_3=192.168.1.30

# fabric image version
FABRIC_BASE_VER=amd64-0.4.18
FABRIC_IMAGE_VER=amd64-1.4.4
FABRIC_CA_VER=amd64-1.4.4

# remote user info
REMOTE_USER=root
REMOTE_PASSWORD=Energy@123


# chaincode configuration
LOCAL_GOPATH=$GOPATH
CHAINCODE_NAME=myfoo
CHAINCODE_VERSION=v1527040507
CHAINCODE_INIT_ARGS={"Args":["init"]}
CHAINCODE_INVOKE_ARGS={"Args":["invoke","admin"]}

CHAINCODE_PACKAGE_REMOTE=energy.com/blockchain/chaincode
CHAINCODE_INSTALL=energy.com/blockchain/chaincode/performance


# multichannel and multiOrg configuration
ORG_PEER_SET={"orgs": "1", "peers": "0 1"}
CHANNEL_SET=[{"cfg":"OneOrgChannel","id":1,"name":"channel1","orgs":"1","peers":"0 1"}]


# all container configuration
TLS_ENABLED=false
CH_ARTIFACTS=./channel-artifacts
CRYPTO_LOCAL=./crypto-config

ORDERER_LIST=orderer
ORDERER_DOMAIN=orderer.net
ORDERER_MSPID=OrdererMSP

PEER_DOMAIN_1=energy.net
PEER_MSPID_1=Org1MSP

PEER_DOMAIN_2=bank2.net
PEER_MSPID_2=Org2MSP

#PEER_DOMAIN_3=bank3.net
#PEER_MSPID_3=Org3MSP
